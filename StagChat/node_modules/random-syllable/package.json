  "author": {
    "name": "bubkoo",
    "email": "bubkoo.wy@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mock-end/random-syllable.git"
  },
  "bugs": {
    "url": "https://github.com/mock-end/random-syllable/issues"
  },
  "homepage": "https://github.com/mock-end/random-syllable#readme",
  "devDependencies": {
    "jshint": "^2.9.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "clamp": "^1.0.1",
