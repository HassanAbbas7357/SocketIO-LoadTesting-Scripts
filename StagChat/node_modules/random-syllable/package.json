{
  "name": "random-syllable",
  "version": "1.0.1",
  "description": "Return a semi-speakable syllable, 2 or 3 letters.",
  "main": "index.js",
  "scripts": {
    "lint": "jshint index.js",
    "pretest": "npm run lint",
    "test": "mocha -R spec",
